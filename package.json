{
    "name": "typescript-aws-helpers",
    "version": "2.0.0",
    "description": "Collection of AWS Lambda Helpers",
    "repository": {
        "type": "git",
        "url": "https://github.com/kbrashears5/typescript-aws-helpers.git"
    },
    "bugs": {
        "url": "https://github.com/kbrashears5/typescript-aws-helpers/issues"
    },
    "homepage": "https://github.com/kbrashears5/typescript-aws-helpers#readme",
    "author": "Kevin Brashears",
    "keywords": [
        "amazon",
        "apigateway",
        "aws",
        "cloudwatch",
        "dynamo",
        "helper",
        "javascript",
        "kms",
        "lambda",
        "orchestrate",
        "s3",
        "ses",
        "sns",
        "sqs",
        "ssm",
        "sts",
        "typescript"
    ],
    "main": "lib/index.js",
    "license": "MIT",
    "dependencies": {
        "request": "^2.88.0",
        "typescript-aws-apigateway-helper": "^1.0.3",
        "typescript-aws-cloudwatch-helper": "^1.0.2",
        "typescript-aws-dynamo-helper": "^1.0.2",
        "typescript-aws-kms-helper": "^1.0.2",
        "typescript-aws-lambda-helper": "^1.0.2",
        "typescript-aws-s3-helper": "^1.0.2",
        "typescript-aws-ses-helper": "^1.0.2",
        "typescript-aws-sns-helper": "^1.0.2",
        "typescript-aws-sqs-helper": "^1.0.2",
        "typescript-aws-ssm-helper": "^1.0.3",
        "typescript-aws-sts-helper": "^1.0.2",
        "typescript-helper-functions": "^1.0.9",
        "typescript-ilogger": "^1.0.3",
        "uuid": "^8.2.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.59",
        "@types/node": "^14.0.22",
        "aws-sdk": "^2.713.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "tslint": "^6.1.2",
        "typescript": "^3.9.6"
    },
    "files": [
        "lib/**"
    ],
    "scripts": {
        "prebuild": "rimraf ./lib",
        "build": "./node_modules/.bin/tsc",
        "clean": "rimraf ./lib",
        "prepublishOnly": "npm run build",
        "postpublish": "npm run clean",
        "test": "echo \"Error: no test specified\" && exit 1",
        "update": "run-s update:*",
        "update:apigateway_helper": "npm i typescript-aws-apigateway-helper@latest",
        "update:cloudwatch_helper": "npm i typescript-aws-cloudwatch-helper@latest",
        "update:dynamo_helper": "npm i typescript-aws-dynamo-helper@latest",
        "update:kms_helper": "npm i typescript-aws-kms-helper@latest",
        "update:lambda_helper": "npm i typescript-aws-lambda-helper@latest",
        "update:s3_helper": "npm i typescript-aws-s3-helper@latest",
        "update:ses_helper": "npm i typescript-aws-ses-helper@latest",
        "update:sns_helper": "npm i typescript-aws-sns-helper@latest",
        "update:sqs_helper": "npm i typescript-aws-sqs-helper@latest",
        "update:ssm_helper": "npm i typescript-aws-ssm-helper@latest",
        "update:sts_helper": "npm i typescript-aws-sts-helper@latest",
        "update:helper_functions": "npm i typescript-helper-functions@latest",
        "update:ilogger": "npm i typescript-ilogger@latest",
        "update:uuid": "npm i uuid@latest",
        "update:types_lambda": "npm i @types/aws-lambda@latest --save-dev",
        "update:types_node": "npm i @types/node@latest --save-dev",
        "update:aws_sdk": "npm i aws-sdk@latest --save-dev",
        "update:npm_run_all": "npm i npm-run-all@latest --save-dev",
        "update:rimraf": "npm i rimraf@latest --save-dev",
        "update:tslint": "npm i tslint@latest --save-dev",
        "update:typescript": "npm i typescript@latest --save-dev"
    }
}
